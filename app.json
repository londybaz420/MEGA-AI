{
  "name": "whatsapp-Automation",
  "description": " Whatsapp Ai",
  "keywords": [
    "whatsapp Ai"
  ],
  "repository": "https://github.com/GlobalTechInfo/MEGA-AI",
  "stack": "container",
  "env": {
    "OWNERS": {
      "description": "Owners Phone Number",
      "required": true,
      "value": "923204566006"
    },
    "SESSION_ID": {
      "description": "ENTER SESSION_ID STARTS WITH EDITH-MD~",
      "required": true,
      "value": ""
    },
    "BOTNAME": {
      "description": "Give your BOT a name",
      "required": false,
      "value": "MEGA-AI"
    },
    "MONGODB_URI": {
      "description": "your mongodb database url",
      "required": true,
      "value": ""
    },
    "MODE": {
      "description": "mode public or private",
      "required": true,
      "value": ""
    },
    "PREFIX": {
      "description": "put any one symbol here except @ and + , leave it Blank if you want to use multiple prefix",
      "required": false,
      "value": ""
    },
    "statusview": {
      "description": "make it true if you want bot to view status",
      "required": false,
      "value": "false"
    },
    "antidelete": {
      "description": "bot will forward deleted messages if you make it true",
      "required": false,
      "value": "false"
    },
    "HOBBIES": {
      "description": "owners hobby for intro card",
      "required": false,
      "value": "CODING"
    },
    "LANGUAGE": {
      "description": "owners native language for intro",
      "required": false,
      "value": "ENGLISH"
    },
    "OWNER_SKILLS": {
      "description": "skills for intro",
      "required": false,
      "value": "JAVASCRIPT"
    },
    "OWNER_STATUS": {
      "description": "current status for intro",
      "required": false,
      "value": "DEVELOPER"
    },
    "TIME_ZONE": {
      "description": "your time zone",
      "required": true,
      "value": "Africa/Nairobi"
    },
    "OWNER_NAME": {
      "description": "your owner Name",
      "required": false,
      "value": ""
    },
     "REMOVEBG_KEY": {
      "description": "your Remove background api key",
      "required": false,
      "value": ""
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
